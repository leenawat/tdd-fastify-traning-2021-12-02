{
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "@typescript-eslint/indent": [
            "error",
            2
        ],
        "semi": [
            "error",
            "never"
        ],
        "quotes": [
            2,
            "single"
        ]
    }
}